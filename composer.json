{
  "name": "abbadon1334/atk4-session",
  "description": "php session using atk4 data handler",
  "keywords": ["session_save_handler", "atk4", "data", "dsql"],
  "minimum-stability": "dev",
  "license": "MIT",
  "authors": [
    {
      "name": "Francesco Danti",
      "email": "fdanti@gmail.com"
    }
  ],
  "require": {
    "atk4/data": "2.4.0",
    "php": ">7.3.0",
    "ramsey/uuid": "4.x-dev"
  },
  "require-dev": {
    "guzzlehttp/guzzle": "6.3.3",
    "friendsofphp/php-cs-fixer": "3.0.0",
    "phpstan/phpstan": "0.12.94",
    "phpunit/phpunit": "9.5.10",
    "phpunit/phpcov": "*",
    "symfony/process": "*",
    "codeclimate/php-test-reporter": "*",
    "nikic/fast-route": "1.3.0"
  },
  "autoload": {
    "psr-4": {
      "Atk4\\ATK4DBSession\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Atk4\\ATK4DBSession\\Tests\\" : "tests"
    }
  },
  "scripts": {
    "fix": "php-cs-fixer fix --config=.php_cs.dist demos src tests"
  }
}
